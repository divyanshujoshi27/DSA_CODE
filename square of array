#include<iostream>
using namespace std;
int arr_sqr(int arr[],int number)
{
    for(int i=0;i<number;i++)
    {
        arr[i]=(arr[i])*(arr[i]);
    }
    
    for(int i=0;i<number;i++)
    {
        cout<<arr[i]<<endl;
    }
}
int main()
{
    int number;
    cin>>number;
    int arr[number];
    for(int i=0;i<number;i++)
    {
        cin>>arr[i];
        
    }
    
    arr_sqr(arr,number);
    
    return 0;
    
}
